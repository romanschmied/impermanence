Cell[BoxData[{
 RowBox[{"MakeImpermanentVariable", "[", 
  RowBox[{"x", ",", "1"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"t0", "=", 
   RowBox[{"AbsoluteTime", "[", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"T", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Pause", "[", "0.01", "]"}], ";", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"AbsoluteTime", "[", "]"}], "-", "t0"}], ",", "x"}], "}"}]}],
      ",", 
     RowBox[{"{", "1000", "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{"T", ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"FrameLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<time (s)\>\"", ",", "\"\<x\>\""}], "}"}]}]}], 
  "]"}]}], "Input",
