Cell[TextData[{
 "create an impermanence variable ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "7973ba67-aae2-473f-bfea-1cee9eb646f3"],
 ", initialized to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "4ec4b29e-4d8f-4d0a-8811-37bad9832984"],
 ", that diffuses at a rate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalD]", "=", 
    RowBox[{"0.01", 
